{
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "sourceMap": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2021",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2021",
      "dom",
      "dom.iterable",
    ]
  }
}
